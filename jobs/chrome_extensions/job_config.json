{
  "job_id": "chrome_extensions_scraper",
  "name": "Chrome Web Store Extensions Scraper",
  "description": "Scrapes Chrome Web Store for extension metadata and updates the chrome_extensions table",
  "enabled": true,
  "source": {
    "type": "script",
    "script_path": "scraper.py",
    "working_directory": "jobs/chrome_extensions",
    "timeout_seconds": 7200,
    "retry_attempts": 3,
    "retry_delay_seconds": 300
  },
  "output": {
    "format": "csv",
    "directory": "results",
    "filename": "chrome_extensions.csv"
  }
}